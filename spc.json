{
    "assets": [
        "mendel_go.css",
        "toml.min.js",
        "mendel_go.js"
    ],
    "category": "",
    "command": "../../../../src/spc_apps/mendel_go/mendel_go -f mendel_go.toml",
    "description": "Go version of Mendel's Accountant",
    "input_format": "toml",
    "language": "",
    "name": "mendel_go",
    "plots": [
        {
            "title": "Average mutations/individual",
            "ptype": "flot-scatter",
            "datasource": [
                {
                    "cols": "1:2",
                    "data_def": "{label: \"deleterious\", data: d1, lines: { show: true }, color: \"rgb(200,0,0)\"}",
                    "filename": "mendel.hst",
                    "label": "deleterious",
                    "line_range": "2:10000"
                },
                {
                    "cols": "1:3",
                    "data_def": "{label: \"neutral\", data: d2, lines: { show: true }, color: \"rgb(0,0,200)\"}",
                    "filename": "mendel.hst",
                    "label": "neutral",
                    "line_range": "2:10000"
                },
                {
                    "cols": "1:4",
                    "data_def": "{label: \"favorable\", data: d3, lines: { show: true }, color: \"rgb(0,200,0)\"}",
                    "filename": "mendel.hst",
                    "label": "favorable",
                    "line_range": "2:10000"
                }
            ],
            "options": "legend: { position: 'nw' }, xaxis: { min: 1.0, axisLabel: 'Generations', axisLabelFontSizePixels: 12 }, yaxis: { axisLabel: 'Mutations', axisLabelFontSizePixels: 12 }, grid: { hoverable: true, clickable: true }, selection: { mode: 'xy' }"
        },
        {
            "title": "Fitness history",
            "ptype": "flot-scatter",
            "datasource": [
                {
                    "cols": "1:4",
                    "data_def": "{label: \"fitness\", data: d1, lines: { show: true }, color: \"rgb(200,0,0)\"}",
                    "filename": "mendel.fit",
                    "label": "fitness",
                    "line_range": "2:10000"
                },
                {
                    "cols": "1:2",
                    "data_def": "{label: 'pop size', data: d2, lines: { show: true}, color: 'rgb(0,0,255)', yaxis: 2, position: 'right'}",
                    "filename": "mendel.fit",
                    "label": "pop size",
                    "line_range": "2:10000"
                }
            ],
            "options": "xaxes: [ { axisLabel: 'Generations', axisLabelFontSizePixels: 12 } ],  yaxes: [ { min: 0, axisLabel: 'Fitness', axisLabelFontSizePixels: 12 }, { position: 'right', min: 0, axisLabel: 'Population Size', axisLabelFontSizePixels: 12} ], legend: { position: 'se'}, grid: { hoverable: true, clickable: true }, selection: { mode: 'xy' }"
        },
        {
            "title": "Distribution of accumulated mutations (deleterious)",
            "ptype": "flot-3d",
            "options": {
                "directory": "allele-distribution-del",
                "z_property": "generation",
                "z_label": "Generation",
                "datasources": [
                    {
                        "x_property": "binmidpointfitness",
                        "y_property": "dominant",
                        "data_def": {
                            "label": "Dominant",
                            "color": "rgb(0, 200, 0)"
                        }
                    },
                    {
                        "x_property": "binmidpointfitness",
                        "y_property": "recessive",
                        "data_def": {
                            "label": "Recessive",
                            "color": "rgb(200, 0, 0)"
                        }
                    }
                ],
                "flot_options": {
                    "xaxis": {
                        "axisLabel": "Mutational Fitness Degradation",
                        "axisLabelFontSizePixels": 12,
                        "ticks": [
                            [1.0, "1"],
                            [0.1, "1e-1"],
                            [0.01, "1e-2"],
                            [1e-3, "1e-3"],
                            [1e-4, "1e-4"],
                            [1e-5, "1e-5"],
                            [1e-6, "1e-6"],
                            [1e-7, "1e-7"],
                            [1e-8, "1e-8"]
                        ],
                        "tickDecimals": 6,
                        "max": 1
                    },
                    "yaxis": {
                        "axisLabel": "Fraction of Mutations Retained in Genome",
                        "axisLabelFontSizePixels": 12
                    },
                    "legend": {
                        "position": "ne"
                    },
                    "series": {
                        "stack": true,
                        "bars": {
                            "show": true,
                            "autoWidth": true
                        }
                    },
                    "selection": {
                        "mode": "xy"
                    }
                },
                "x_axis_scale": "negative_log"
            }
        },
        {
            "title": "Distribution of accumulated mutations (beneficial)",
            "ptype": "flot-3d",
            "options": {
                "directory": "allele-distribution-fav",
                "z_property": "generation",
                "z_label": "Generation",
                "datasources": [
                    {
                        "x_property": "binmidpointfitness",
                        "y_property": "dominant",
                        "data_def": {
                            "label": "Dominant",
                            "color": "rgb(0, 200, 0)"
                        }
                    },
                    {
                        "x_property": "binmidpointfitness",
                        "y_property": "recessive",
                        "data_def": {
                            "label": "Recessive",
                            "color": "rgb(200, 0, 0)"
                        }
                    }
                ],
                "flot_options": {
                    "xaxis": {
                        "axisLabel": "Mutational Fitness Enhancement",
                        "axisLabelFontSizePixels": 12,
                        "ticks": [
                            [1.0, "1"],
                            [0.1, "1e-1"],
                            [0.01, "1e-2"],
                            [1e-3, "1e-3"],
                            [1e-4, "1e-4"],
                            [1e-5, "1e-5"],
                            [1e-6, "1e-6"],
                            [1e-7, "1e-7"],
                            [1e-8, "1e-8"]
                        ],
                        "tickDecimals": 6
                    },
                    "yaxis": {
                        "axisLabel": "Fraction of Mutations Retained in Genome",
                        "axisLabelFontSizePixels": 12
                    },
                    "legend": {
                        "position": "ne"
                    },
                    "series": {
                        "stack": true,
                        "bars": {
                            "show": true,
                            "autoWidth": true
                        }
                    },
                    "selection": {
                        "mode": "xy"
                    }
                },
                "x_axis_scale": "log"
            }
        },
        {
            "title": "SNP Frequencies",
            "ptype": "flot-3d",
            "options": {
                "directory": "allele-bins",
                "z_property": "generation",
                "z_label": "Generation",
                "datasources": [
                    {
                        "x_property": "bins",
                        "y_property": "deleterious",
                        "data_def": {
                            "label": "Deleterious",
                            "color": "rgb(200, 0, 0)"
                        }
                    },
                    {
                        "x_property": "bins",
                        "y_property": "favorable",
                        "data_def": {
                            "label": "Favorable",
                            "color": "rgb(0, 200, 0)"
                        }
                    },
                    {
                        "x_property": "bins",
                        "y_property": "neutral",
                        "data_def": {
                            "label": "Neutral",
                            "color": "rgb(0, 0, 200)"
                        }
                    },
                    {
                        "x_property": "bins",
                        "y_property": "delInitialAlleles",
                        "data_def": {
                            "label": "Deleterious Initial",
                            "color": "rgb(237, 158, 0)"
                        }
                    },
                    {
                        "x_property": "bins",
                        "y_property": "favInitialAlleles",
                        "data_def": {
                            "label": "Favorable Initial",
                            "color": "rgb(200, 0, 200)"
                        }
                    }
                ],
                "flot_options": {
                    "xaxis": {
                        "axisLabel": "SNP Frequencies",
                        "axisLabelFontSizePixels": 12
                    },
                    "yaxes": {
                        "axisLabel": "Number of Alleles",
                        "axisLabelFontSizePixels": 12
                    },
                    "legend": {
                        "position": "ne"
                    },
                    "series": {
                        "stack": true,
                        "bars": {
                            "show": true,
                            "lineWidth": 0,
                            "barWidth": 0.9,
                            "fill": 0.8
                        }
                    },
                    "selection": {
                        "mode": "xy"
                    }
                }
            }
        },
        {
            "title": "Minor Allele Frequencies",
            "ptype": "flot-3d",
            "options": {
                "directory": "normalized-allele-bins",
                "z_property": "generation",
                "z_label": "Generation",
                "datasources": [
                    {
                        "x_property": "bins",
                        "y_property": "deleterious",
                        "data_def": {
                            "label": "Deleterious",
                            "color": "rgb(200, 0, 0)"
                        }
                    },
                    {
                        "x_property": "bins",
                        "y_property": "favorable",
                        "data_def": {
                            "label": "Favorable",
                            "color": "rgb(0, 200, 0)"
                        }
                    },
                    {
                        "x_property": "bins",
                        "y_property": "neutral",
                        "data_def": {
                            "label": "Neutral",
                            "color": "rgb(0, 0, 200)"
                        }
                    },
                    {
                        "x_property": "bins",
                        "y_property": "delInitialAlleles",
                        "data_def": {
                            "label": "Deleterious Initial",
                            "color": "rgb(237, 158, 0)"
                        }
                    },
                    {
                        "x_property": "bins",
                        "y_property": "favInitialAlleles",
                        "data_def": {
                            "label": "Favorable Initial",
                            "color": "rgb(200, 0, 200)"
                        }
                    }
                ],
                "flot_options": {
                    "xaxis": {
                        "axisLabel": "Minor Allele Frequency",
                        "axisLabelFontSizePixels": 12
                    },
                    "yaxis": {
                        "axisLabel": "Proportion of Minor SNPs",
                        "axisLabelFontSizePixels": 12
                    },
                    "legend": {
                        "position": "ne"
                    },
                    "series": {
                        "stack": true,
                        "bars": {
                            "show": true,
                            "lineWidth": 0,
                            "barWidth": 0.9,
                            "fill": 0.8
                        }
                    },
                    "selection": {
                        "mode": "xy"
                    }
                }
            }
        }
    ],
    "postprocess": "",
    "preprocess": ""
}
